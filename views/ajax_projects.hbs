<div class="row" id="projetos">
{{#each object_projects}}
      
      <div class="col-xl-3 col-md-6 mb-xl-0 mb-4">
                    <div class="card card-blog card-plain">
                      
                      <div class="card-body p-3">
                        <p class="mb-0 text-sm">Project {{@index}}</p>
                        <a href="javascript:;">
                          <h5>
                            {{this.nome}}
                          </h5>
                        </a>
                        <p class="mb-4 text-sm">
                          {{this.bio}}
                        </p>
                        <div class="d-flex align-items-center justify-content-between">
                          <a href="../projects/{{this}}"
                          <button type="button" class="btn btn-outline-primary btn-sm mb-0">Acompanhar projeto</button>
                          </a>
                        
                        </div>
                      </div>
                    </div>
                  </div>
    {{/each}}
    </div>